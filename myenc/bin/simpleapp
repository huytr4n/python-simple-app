#!/Users/huytran/workspace/projects/data-science/pythonapp/myenc/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'simpleapp','console_scripts','simpleapp'
__requires__ = 'simpleapp'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('simpleapp', 'console_scripts', 'simpleapp')()
    )
